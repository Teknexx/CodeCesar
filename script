#include <stdio.h>
#include <stdlib.h>

unsigned char mot[] = 
{    //inserez ici la chaine a decrypter, en hexadecimal
  0x4c, 0x7c, 0x6b, 0x80, 0x79, 0x2b
};



int main() {
  int longueur = 6; //indiquez ici la longueur du message
  int i,j,c;
  for (i = 0; i < 256; i++){
    for (j = 0; j < longueur; j++){
      c = mot[j]-i;
      printf("%c", c);
    }
    printf("\n");
  }
  return 0;
}
